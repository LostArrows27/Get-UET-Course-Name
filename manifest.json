{
    "name": "UET Course's Name",
    "description": "Adjust UET Course's name to subject name",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "js": [
                "changeName.js"
            ],
            "matches": [
                "https://courses.uet.vnu.edu.vn/*"
            ]
        }
    ],
    "action": {
        "default_icon": {
            "16": "/images/16.png",
            "32": "/images/32.png",
            "48": "/images/48.png",
            "128": "/images/128.png"
        }
    },
    "icons": {
            "16": "/images/16.png",
            "32": "/images/32.png",
            "48": "/images/48.png",
            "128": "/images/128.png"
    }
}